        <div class="nav navbar-nav navbar-right">
          <span class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true" id="dropdownMenu1">
                <img src="http://placehold.it/35x35" alt="avatar" class="img-circle" />
                <span class="caret"></span>
              </a>
              
            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
              <li role="presentation"><a role="menuitem" tabindex="-1" href="../HTML/dashboard-settings.html">Settings</a></li>
