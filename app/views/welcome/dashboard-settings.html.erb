    <div ng-view></div>

    <div class="row">
      <!-- Filler space for accordion-->
      <div class="col-md-2 sidebar">
        <!--<button class="btn btn-default btn-lg btn-block">Settings</button>
        <button class="btn btn-default btn-lg btn-block">Subscription</button>
        <button class="btn btn-default btn-lg btn-block">Events</button>
        <button class="btn btn-default btn-lg btn-block">Groups</button>-->
        <div class="link-group text-center">
          <a href="../HTML/dashboard-settings.html" class="link">Settings</a>
          <a href="#" class="link">Subscription</a>
          <a href="#" class="link">My Events</a>
          <a href="#" class="link">My Groups</a>
        </div>
      </div>

      <!-- Main div -->
      <div class="col-md-10">
        <h1>Account Dashboard</h1>
        <ul class="breadcrumb">
          <li><a href="../HTML/dashboard-settings.html">Account Dashboard</a></li>
          <li><a href="../HTML/dashboard-settings.html">Settings</a></li>
        </ul>

        <div class="row">
          <div class="col-md-10">
            <!-- First form -->
            <form class="form-horizontal form-fieldset">
              <fieldset>
                <legend>Account Information</legend>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Full Name:</label>
                  <div class="col-md-5">
                    <input type="text" class="form-control" id="inputEmail3" placeholder="Enter name...">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Email:</label>
                  <div class="col-md-5">
                    <input type="email" class="form-control" id="inputEmail3" placeholder="Enter email...">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Password:</label>
                  <div class="col-md-5">
                    <input type="password" class="form-control" id="inputEmail3" placeholder="*******">
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-md-2"></div>
                  <p class="help-block col-md-5">
                    <a href="" class="text-right">Change password</a>
                  </p>
                </div>
                <div class="pull-right">
                  <button class="btn btn-success green">Save</button>
                </div>
              </fieldset>
            </form>
            <div class="clear-space"></div>
            <!--second form-->
            <!-- First form -->
            <form class="form-horizontal form-fieldset">
              <fieldset>
                <legend>Profile Information</legend>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Profile Picture:</label>
                  <div class="col-md-5">
                    <input type="file" class="form-control" id="inputEmail3">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Bio:</label>
                  <div class="col-md-5">
                    <textarea class="form-control"></textarea>
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Social Networks:</label>
                  <div class="col-md-5">
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Facebook
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Twitter
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> YouTube
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Blogger
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Pinterest
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Tumblr
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                    <div class="checkbox">
                      <label>
                        <input type="checkbox" class="checkbox" id="inlineCheckbox3" value="option3"> Website
                        <!--<input type="text" class="form-control" placeholder="www.facebook.com" />-->
                      </label>
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label for="inputEmail3" class="col-md-2 control-label">Show events publicly:</label>
                  <div class="col-md-5">
                    <div class="radio">
                      <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
                        Show events on profile to all members
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1">
                        Show events on profile to only me
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1">
                        Don't show events on profile
                      </label>
                    </div>
                  </div>
                </div>
               
                <div class="pull-right">
                  <button class="btn btn-success green">Save</button>
                </div>
              </fieldset>
            </form>
          </div>
        </div>
      </div>
    </div>
    <div class="clear-space"></div>

      
  </body>

</html>
